<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="#f5f5f5"
    android:layout_height="match_parent">
    <include layout="@layout/layout_title_bar_com"/>

        <RelativeLayout
            android:background="#fff"
            android:id="@+id/cv_bna"
            android:layout_marginTop="@dimen/dp_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/dp_8">
            <ImageView
                android:layout_marginLeft="@dimen/dp_10"
                android:src="@drawable/bna"
                android:id="@+id/iv_pic"
                android:layout_centerVertical="true"
                android:layout_width="@dimen/dp_55"
                android:layout_height="@dimen/dp_55" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:layout_alignTop="@+id/iv_pic"
                android:layout_marginLeft="@dimen/dp_18"
                android:layout_marginStart="@dimen/dp_18"
                android:layout_marginTop="@dimen/dp_8"
                android:layout_toEndOf="@+id/iv_pic"
                android:layout_toRightOf="@+id/iv_pic"
                android:text="发型历史记录"
                android:textColor="#333"
                android:id="@+id/textView" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_pic"
                android:layout_alignLeft="@+id/textView"
                android:layout_alignStart="@+id/textView"
                android:layout_marginBottom="@dimen/dp_11"
                android:text="随时随地查看历史发型"
                android:textColor="#666"
                android:textSize="@dimen/sp_12" />
            <ImageView
                android:layout_width="@dimen/dp_15"
                android:layout_height="@dimen/dp_25"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="19dp"
                android:layout_marginRight="@dimen/dp_19"
                android:src="@mipmap/ic_right" />


        </RelativeLayout>





        <RelativeLayout
            android:background="#fff"
            android:layout_width="match_parent"
            android:id="@+id/cv_skulp"
            android:layout_height="wrap_content"
            android:padding="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_6">
            <ImageView
                android:layout_marginLeft="@dimen/dp_10"
                android:src="@drawable/skulp"
                android:id="@+id/iv_pic_skin"
                android:layout_centerVertical="true"
                android:layout_width="@dimen/dp_55"
                android:layout_height="@dimen/dp_55" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:layout_alignTop="@+id/iv_pic_skin"
                android:layout_marginLeft="@dimen/dp_18"
                android:layout_marginStart="@dimen/dp_18"
                android:layout_marginTop="@dimen/dp_8"
                android:layout_toEndOf="@+id/iv_pic_skin"
                android:layout_toRightOf="@+id/iv_pic_skin"
                android:text="头皮管理历史记录"
                android:textColor="#333"
                android:id="@+id/textView_skin" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_pic_skin"
                android:layout_alignLeft="@+id/textView_skin"
                android:layout_alignStart="@+id/textView_skin"
                android:layout_marginBottom="@dimen/dp_11"
                android:text="随时随地查看头皮历史状况"
                android:textColor="#666"
                android:textSize="@dimen/sp_12" />
            <ImageView
                android:layout_width="@dimen/dp_15"
                android:layout_height="@dimen/dp_25"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="19dp"
                android:layout_marginRight="19dp"
                android:src="@mipmap/ic_right" />


        </RelativeLayout>


        <RelativeLayout
            android:background="#fff"
            android:id="@+id/cv_skin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_6">
            <ImageView
                android:layout_marginLeft="@dimen/dp_10"
                android:src="@drawable/skin"
                android:id="@+id/iv_pic_skulp"
                android:layout_centerVertical="true"
                android:layout_width="@dimen/dp_55"
                android:layout_height="@dimen/dp_55" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/sp_16"
                android:layout_alignTop="@+id/iv_pic_skulp"
                android:layout_marginLeft="@dimen/dp_18"
                android:layout_marginStart="@dimen/dp_18"
                android:layout_marginTop="@dimen/dp_8"
                android:layout_toEndOf="@+id/iv_pic_skulp"
                android:layout_toRightOf="@+id/iv_pic_skulp"
                android:text="皮肤管理历史记录"
                android:textColor="#333"
                android:id="@+id/textView_skulp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_pic_skulp"
                android:layout_alignLeft="@+id/textView_skulp"
                android:layout_alignStart="@+id/textView_skulp"
                android:layout_marginBottom="@dimen/dp_11"
                android:text="随时随地查看皮肤历史状况"
                android:textColor="#666"
                android:textSize="@dimen/sp_12" />

            <ImageView
                android:layout_width="@dimen/dp_15"
                android:layout_height="@dimen/dp_25"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_19"
                android:layout_marginRight="@dimen/dp_19"
                android:src="@mipmap/ic_right" />
        </RelativeLayout>
</LinearLayout>